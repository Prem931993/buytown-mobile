    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.premkumar.buytown",
      "config": {
        "googleMapsApiKey": "AIzaSyDao7PSifrnHW0ly7XDAHASdb5wJneSSPQ"
      },
      "infoPlist": {
