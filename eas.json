{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_ENV": "Development",
        "EXPO_PUBLIC_API_URL": "https://buytown-production.up.railway.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_ENV": "Preview",
        "EXPO_PUBLIC_API_URL": "https://buytown-production.up.railway.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_ENV": "Production",
        "EXPO_PUBLIC_API_URL": "https://buytown-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
